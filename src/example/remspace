package example;

public class remspace {
	public static void main(String args[]){
	        String str="My name is Anees";
	        String result="";
	        for(int i=0;i<str.length();i++){
	            if(str.charAt(i)!=' '){
	                    result+=str.charAt(i);
	            }
	        }
	        System.out.println("The string without space is "+result);
	    }
	}
